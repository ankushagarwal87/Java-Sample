<?xml version="1.0"?>

<!DOCTYPE weblogic-rdbms-jar PUBLIC 
 '-//BEA Systems, Inc.//DTD WebLogic 7.0.0 EJB RDBMS Persistence//EN'
 'http://www.bea.com/servers/wls810/dtd/weblogic-rdbms20-persistence-810.dtd'>

<weblogic-rdbms-jar>
  <weblogic-rdbms-bean>
    <ejb-name>EmpEjb</ejb-name>
    <data-source-name>ankush</data-source-name>
    <table-map>
      <table-name>Emp</table-name>
      <field-map>
        <cmp-field>eid</cmp-field>
        <dbms-column>id</dbms-column>
      </field-map>
      <field-map>
        <cmp-field>ename</cmp-field>
        <dbms-column>name</dbms-column>
      </field-map>
    </table-map>
  </weblogic-rdbms-bean>
  <create-default-dbms-tables>True</create-default-dbms-tables>
</weblogic-rdbms-jar>

